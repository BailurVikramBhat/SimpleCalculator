<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <item
        android:state_pressed="false"
        >

        <shape>
            <corners
                android:topLeftRadius="80dp"
                android:topRightRadius="80dp"
                android:bottomLeftRadius="80dp"
                android:bottomRightRadius="80dp"
                />
            <gradient
                android:angle="45"
                android:centerX="35%"
                android:centerColor="#fffe9500"
                android:type="linear"

                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />



            <solid android:color="#323232" />

        </shape>
    </item>

    <item
        android:state_focused="false"
        >

        <shape>
            <corners
                android:topLeftRadius="80dp"
                android:topRightRadius="80dp"
                android:bottomLeftRadius="80dp"
                android:bottomRightRadius="80dp"
                />
            <gradient
                android:angle="45"
                android:centerX="35%"
                android:centerColor="#fffe9500"
                android:type="linear"

                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />



            <solid android:color="#fe9500" />

        </shape>
    </item>
</selector>